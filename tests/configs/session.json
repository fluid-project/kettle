{
    "typeName": "session",
    "options": {
        "gradeNames": ["fluid.component"],
        "components": {
            "server": {
                "type": "kettle.server",
                "options": {
                    "gradeNames": ["kettle.use.session", "kettle.server.io", "kettle.tests.sessionServer"],
                    "components": {
                        "app": {
                            "type": "kettle.app",
                            "options": {
                                "handlers": {
                                    "testNoneSessionRequest": {
                                        "route": "/testNoneSessionRequest",
                                        "type": "get",
                                        "useSession": "none"
                                    },
                                    "testNoSessionRequest": {
                                        "route": "/testNoSessionRequest",
                                        "type": "get"
                                    },
                                    "testSessionSocket": {
                                        "route": "/testSessionSocket",
                                        "type": "io",
                                        "useSession": "existing"
                                    },
                                    "testSessionRequest": {
                                        "route": "/testSessionRequest",
                                        "type": "get",
                                        "useSession": "existing"
                                    },
                                    "testSessionStart": {
                                        "route": "/testSessionStart/:token",
                                        "type": "get",
                                        "useSession": "create"
                                    },
                                    "testSessionEnd": {
                                        "route": "/testSessionEnd/:token",
                                        "type": "get",
                                        "useSession": "create"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
